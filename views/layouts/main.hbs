<body>
    {{>header}}
    {{{body}}}
    {{>footer}}

    <script src="https://themepixels.me/demo/bracket/lib/jquery/jquery.js"></script>
    <script src="https://themepixels.me/demo/bracket/lib/popper.js/popper.js"></script>
    <script src="https://themepixels.me/demo/bracket/lib/bootstrap/bootstrap.js"></script>
    <script src="https://themepixels.me/demo/bracket/lib/perfect-scrollbar/js/perfect-scrollbar.jquery.js"></script>
    <script src="https://themepixels.me/demo/bracket/lib/moment/moment.js"></script>
    <script src="https://themepixels.me/demo/bracket/lib/jquery-ui/jquery-ui.js"></script>
    <script src="https://themepixels.me/demo/bracket/lib/jquery-switchbutton/jquery.switchButton.js"></script>
    <script src="https://themepixels.me/demo/bracket/lib/peity/jquery.peity.js"></script>
    <script src="https://themepixels.me/demo/bracket/lib/highlightjs/highlight.pack.js"></script>
    <script src="https://themepixels.me/demo/bracket/lib/datatables/jquery.dataTables.js"></script>
    <script src="https://themepixels.me/demo/bracket/lib/datatables-responsive/dataTables.responsive.js"></script>
    <script src="https://themepixels.me/demo/bracket/lib/select2/js/select2.min.js"></script>

    <script src="https://themepixels.me/demo/bracket/js/bracket.js"></script>
    <script>
      $(function(){
        'use strict'

        $('.form-layout .form-control').on('focusin', function(){
          $(this).closest('.form-group').addClass('form-group-active');
        });

        $('.form-layout .form-control').on('focusout', function(){
          $(this).closest('.form-group').removeClass('form-group-active');
        });

        // Select2
        $('#select2-a, #select2-b').select2({
          minimumResultsForSearch: Infinity
        });

        $('#select2-a').on('select2:opening', function (e) {
          $(this).closest('.form-group').addClass('form-group-active');
        });

        $('#select2-a').on('select2:closing', function (e) {
          $(this).closest('.form-group').removeClass('form-group-active');
        });

      });
    </script>
  

  
  
</body>
</html>


